pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
        stage('build') {
            steps {
                sh "curl www.google.com > googleHome.html" 
            }
        }
    }
    post{
    	always{
		archiveArtifacts archive: './'
	}
    }
}
